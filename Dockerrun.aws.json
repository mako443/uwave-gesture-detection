    },
    "containerDefinitions": [
        {
            "name": "uwave",
            "image": "imxdev/uwave",
            "memory": 128,
            "hostname": "uwave",
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ]
        }
    ]
}